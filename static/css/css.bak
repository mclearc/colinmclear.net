html {
  position: relative;
  min-height: 100%;
}

body {
    font-family: 'Helvetica',sans-serif;
    font-size: 17px; 
    line-height: 1.628571; 
    color: black;
  margin-bottom: 70px;

  }

footer {
  font-size: 12px;
}
.footer {
  position: absolute;
  bottom: -5%;
  width: 100%;
  text-align: left;
  padding-bottom: 1rem;
}


p {
    padding-bottom: 5px;
    text-align: justify;

}


